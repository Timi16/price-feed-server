{
  "name": "debonk-price-feed-server",
  "version": "1.0.0",
  "description": "Real-time crypto price feed server using Avantis SDK",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "prepare": "npm run build",
    "prod": "npx pm2 start dist/index.js --name debonk-price-feed-server",
    "delete": "npx pm2 delete debonk-price-feed-server",
    "logs": "npx pm2 logs debonk-price-feed-server"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ethers": "^6.9.0",
    "express": "^4.18.2",
    "perps-sdk-ts": "file:PerpSdk",
    "ws": "^8.14.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/node": "^20.19.26",
    "@types/ws": "^8.5.8",
    "tsx": "^4.6.2",
    "typescript": "^5.2.2"
  }
}
